<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.nwafu.paper.mapper.PaperMapper">
    <select id="selectWithUserId" resultMap="BaseResultMap">
        select * from paper where creator_id = #{userId}
    </select>
    <select id="selectAll" resultMap="BaseResultMap">
        select * from paper
    </select>

    <select id="selectWithStat" resultMap="BaseResultMap">
        select * from paper where state=#{stat}
    </select>
</mapper>